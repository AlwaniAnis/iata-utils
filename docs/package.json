{
  "name": "iata-utils",
  "version": "1.0.0",
  "description": "Various IATA utilities",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist/bundle.js*",
    "lint": "./node_modules/.bin/eslint *.js \"src/*.js\" \"src/*.jsx\"",
    "lint-fix": "./node_modules/.bin/eslint --fix *.js \"src/*.js\" \"src/*.jsx\"",
    "dev": "webpack-dev-server --open --inline --history-api-fallback --config webpack.dev.js",
    "build-dev": "webpack --progress --colors --config webpack.dev.js",
    "build-prod": "export NODE_ENV=production && webpack -p --progress --colors --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/benct/iata-utils.git"
  },
  "keywords": [
    "iata",
    "optd",
    "util",
    "utilities",
    "search",
    "ben",
    "tomlin"
  ],
  "author": "Ben Christopher Tomlin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benct/iata-utils/issues"
  },
  "homepage": "https://benct.github.io/iata-utils",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "prettier": "^2.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7"
  }
}
